# URL for Supabase instance; required for database connection
SUPABASE_URL="your-supabase-url"

# Public API key for Supabase; necessary for client-side operations
SUPABASE_ANON_KEY="your-supabase-anon-key"

# Secret used for managing user sessions securely; ensure this is kept confidential
SESSION_SECRET="your-session-secret"

# Optional: Default email and password for initial setup or testing. Change these for production use.
DEFAULT_EMAIL="default-email@example.com"
DEFAULT_PASSWORD="default-password"
